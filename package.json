{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "phosphor",
  "title": "Phosphor",
  "description": "Phosphor icons search bar",
  "icon": "command-icon.png",
  "author": "marcos_vinicius",
  "categories": [
    "Applications"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Search Icon",
      "description": "Search Phosphor icons",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@phosphor-icons/core": "^2.0.2",
    "@phosphor-icons/react": "^2.0.10",
    "@raycast/api": "^1.54.0",
    "fuse.js": "^6.6.2",
    "use-debounce": "^9.0.4"
  },
  "devDependencies": {
    "@raycast/eslint-config": "1.0.5",
    "@types/node": "18.8.3",
    "@types/react": "18.0.9",
    "eslint": "^7.32.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
